<EccTable>
    <!-- 
        Bolivia
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
   <EccEntry Ecc="110" Category="0" Condition="2" />
   <EccEntry Ecc="119" Category="0" Condition="2" />
   <EccEntry Ecc="911" Category="0" Condition="2" />

   <EccEntry Ecc="103" Category="0" Condition="2" />
   <EccEntry Ecc="106" Category="0" Condition="2" />
   <EccEntry Ecc="105" Category="0" Condition="2" />
   <EccEntry Ecc="101" Category="0" Condition="2" />

   <EccEntry Ecc="160" Category="0" Condition="2" />
   <EccEntry Ecc="176" Category="0" Condition="2" />
   <EccEntry Ecc="177" Category="0" Condition="2" />
   <EccEntry Ecc="178" Category="0" Condition="2" />
</EccTable>
